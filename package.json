    "eslint": "8.44.0",
    "eslint-config-next": "13.4.9",
    "js-yaml": "^4.1.0",
    "next": "13.5.4",
    "openapi-backend": "^5.10.3",
    "openapi-client-axios": "^7.3.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
